#include <bits/stdc++.h>

using namespace std;

int n,  k, cur, ans;
string s;

int main(){
   cin >> n >> k ;
  for(int i = 0; i < n; i++) {
    cin >> s;
    for(int j = 0;j < s.size();j++){
      if(s[j] == '4' || s[j] == '7')cur++;
    }
    if(cur < k)ans++;
    cur = 0;
  }  
  cout << ans;
}
