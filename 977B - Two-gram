#include<iostream>
using namespace std;

int Call(char* gram,char * str,int n){ 
	int c = 0;
	for(int i = 0; i < n; i++) {
		if(str[i]==gram[0] && str[i+1]==gram[1]) 
		    c++;
	}
	return c;
}


int main() {
	int n,temp,max=0;
	cin>>n;
	char * str = new char [n];
	char * gram=new  char[2];
	char *gram2=new char [2];
	cin>>str;
	
	for(int i = 0; i < n; i++) {
		gram[i] = str[i];
		gram[i+1] = str[i+1];
	}
	
	temp = Call(gram,str,n);
	
	if(temp > max) {
		max = temp;
		gram2[0] = gram[0];
		gram2[1] = gram[1];
	}
	cout<<gram2[0]<<gram2[1]<<endl;
	
	return 0;
}
