#include <iostream>

using namespace std;

int main() {
	int n, x = 0;
	cin >> n;
	char s[3];
	for (int i = 0; i < n; i++) {
		
		for (int j = 0; j < 3; j++) {
			cin >> s[j];
		}
			if (s[1] == '+') {
				x= x+1;
			}
			else
			{
				if (s[1] == '-')
					x=x-1;
				
			}

	}
	cout << x << endl;

	//system("pause");
	return 0;
}
