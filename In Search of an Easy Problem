#include <iostream>
#include<string>
using namespace std;

int main() {
	int n,flag = 0;
	cin>>n;
	string s[n];
	
	for(int i = 0; i < n; i++) {
			cin>>s[i];
	}
	for(int j = 0; j < n; j++) {
	   if(s[j]=="1") {
	    flag = 1;
	    break;
	}
	else {
		flag = 0;
	}
	}
 if(flag == 1) {
 	 cout<<"HARD"<<endl;
 }
 else {
 	cout<<"EASY"<<endl;
 }
	return 0;
}
