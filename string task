#include <iostream>
#include <string>
using namespace std;

int main() {
	string s;
	cin>>s;

	for(int i = 0; i < s.length(); i++) {
		s[i] = tolower(s[i]);
		if(s[i] != 'o' && s[i] != 'y' && s[i] != 'i' && s[i] != 'u' && s[i] != 'e' && s[i]!= 'a') {
			cout<<"."<<s[i];
		}
		else {
			continue;
		}
	}
	return 0;
}
