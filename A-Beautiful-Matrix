#include<iostream>
#include<string>
using namespace std;

int abs(int x) {
	if (x < 1) return -x;
	else return x;
}

int main() {

	int x = 0;
	for (int i = 0; i < 5; i++) {
		for (int j = 0; j < 5; j++) {
			cin >> x;
			if (x == 1)
				cout << abs(i - 2) + abs(j - 2) << endl;
		}
	}
//	system("pause");
	return 0;
}
